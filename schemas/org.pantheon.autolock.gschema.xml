<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema path="/org/pantheon/autolock/"
  id="org.pantheon.autolock"
  gettext-domain="autolock">
    <key name="timeout" type="u">
      <default>0</default>
      <range min="0" max="60" />
      <summary>Timeout to lock the screen.</summary>
      <description>Minutes of inactivity until the screen is locked. 0 disables this.</description>
    </key>
  </schema>
</schemalist>
